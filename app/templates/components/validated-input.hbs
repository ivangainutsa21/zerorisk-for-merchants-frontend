{{#tooltip-on-parent
  event='manual'
  visibility=showErrors
  place='top'
  typeClass='warning'
  tabIndex='-1'
}}
  {{#each errorMessages as |errorMessage|}}
    {{errorMessage}}<br>
  {{/each}}
{{/tooltip-on-parent}}

<label>{{label}}</label>
{{input value=value type=type class="form-control" placeholder=placeholder disabled=disabled focus-out=(action 'setHasInteracted')}}
